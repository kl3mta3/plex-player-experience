{
  "manifest_version": 3,
  "name": "Plex Player Experience",
   "key": "",
  "description": "Custom next episode delay, sleep timer, audio boost and auto skip for intros and credits on Plex Web.",
  "version": "1.5.2",
  "permissions": [
    "storage",
	"scripting"
  ],
  "host_permissions": [
    "https://app.plex.tv/*",
    "https://*.plex.direct/*",
    "http://*.plex.direct/*",
    "http://localhost/*",
    "http://127.0.0.1/*"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.plex.tv/*",
        "https://*.plex.direct/*",
        "http://*.plex.direct/*",
        "http://localhost/*",
        "http://127.0.0.1/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Plex Player Experience",
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "icons": {
    "128": "assets/icon128.png"
  }
}